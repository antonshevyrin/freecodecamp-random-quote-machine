{
	"extends": [
		"react-app",
		"airbnb",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"prettier/react"
	],
	"plugins": ["jsx-a11y", "prettier"],
	"rules": {
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
		"import/no-extraneous-dependencies": [
			"error",
			{ "devDependencies": true }
		],
		"react/jsx-uses-react": 1,
		"react/jsx-uses-vars": 1
	}
}
